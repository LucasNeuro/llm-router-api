# Configurações do Servidor
API_HOST=0.0.0.0
API_PORT=8000

# Configurações de Logging
LOG_LEVEL=INFO
LOG_FORMAT="%(asctime)s - %(name)s - %(levelname)s - %(message)s"
LOG_PATH=api/logs

# Chaves de API
OPENAI_API_KEY=sk-...  # Sua chave da OpenAI (começa com sk-)
GEMINI_API_KEY=...     # Sua chave do Google Gemini
MISTRAL_API_KEY=...    # Sua chave do Mistral
DEEPSEEK_API_KEY=...   # Sua chave do DeepSeek

# Configurações do Supabase
SUPABASE_URL=...       # URL do seu projeto Supabase
SUPABASE_KEY=...       # Chave do seu projeto Supabase

# Configurações do WhatsApp (MegaAPI)
MEGAAPI_INSTANCE_ID=... # ID da sua instância MegaAPI
MEGAAPI_API_KEY=...     # Chave da API MegaAPI
MEGAAPI_TOKEN=...       # Token da MegaAPI

# Other configurations
# ... add other environment variables here ... 